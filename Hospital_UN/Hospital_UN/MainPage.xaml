             xmlns:local="clr-namespace:Hospital_UN"
             x:Class="Hospital_UN.MainPage">

    <ContentPage.Content>
        <StackLayout Orientation="Vertical" Padding="15" VerticalOptions="CenterAndExpand">
            <Button Text="Diagnostico Urinarios" Clicked="OnDiagnosticosUrinariosClick" BackgroundColor= "DodgerBlue"></Button>
            <Button Text="Diagnosticos Cardiacos" Clicked="OnDiagnosticosCardiacosClick" BackgroundColor="DodgerBlue"></Button>
        </StackLayout>
    </ContentPage.Content>

</ContentPage>